                },
                "nprs": {
                    "type": "number"
                },
                "timprs": {
                    "type": "null"
                },
                "obs": {
                    "type": "null"
                },
                "nprobs": {
                    "type": "number"
                },
                "chkmas": {
                    "type": "boolean"
                },
                "nprmas": {
                    "type": "number"
                },
                "perlen": {
                    "type": "null"
                },
                "nstp": {
